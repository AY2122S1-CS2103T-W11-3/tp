@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":ManageMeParser" as ManageMeParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedManageMe" as VersionedManageMe MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> ManageMeParser : parseCommand(undo)
activate ManageMeParser

create UndoCommand
ManageMeParser -> UndoCommand
activate UndoCommand

UndoCommand --> ManageMeParser
deactivate UndoCommand

ManageMeParser --> LogicManager : u
deactivate ManageMeParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoManageMe()
activate Model

Model -> VersionedManageMe : undo()
activate VersionedManageMe

VersionedManageMe -> VersionedManageMe :resetData(ReadOnlyManageMe)
VersionedManageMe --> Model :
deactivate VersionedManageMe

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
